{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-imagick": "*",
        "aws/aws-sdk-php": "^3.214",
        "bugsnag/bugsnag-laravel": "^2.23",
        "codedge/laravel-fpdf": "^1.8",
        "dreamonkey/laravel-cloudfront-url-signer": "~3",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "^1.2",
        "inertiajs/inertia-laravel": "^0.5",
        "ixudra/curl": "^6.22",
        "kalnoy/nestedset": "^6.0",
        "laravel/framework": "^9.5",
        "laravel/scout": "^9.4",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.4",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.5",
        "meilisearch/meilisearch-php": "^0.23",
        "mhmiton/laravel-modules-livewire": "^1.5",
        "milon/barcode": "^9.0",
        "mpdf/mpdf": "^8.0",
        "nwidart/laravel-modules": "^9.0",
        "spatie/laravel-medialibrary": "^10.1",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-validation-rules": "^3.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.12",
        "bennett-treptow/laravel-migration-generator": "^4.2",
        "brianium/paratest": "^6.4",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.8",
        "laravel/dusk": "^6.22",
        "laravel/sail": "^1.13",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        },
        "files": [
            "app/Helpers/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    }
}
